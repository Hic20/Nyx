{
  "name": "theme-nyx",
  "displayName": "Nyx",
  "version": "0.0.1",
  "description": "A dark theme inspired by the colors of Nyx.",
  "publisher": "Hic20",
  "license": "MIT",
  "scripts": {
    "build": "node ./scripts/build.js",
    "lint": "node ./scripts/lint.js",
    "package": "vsce package -o ./bin/nyx.vsix",
    "vscode:prepublish": "npm run build",
    "vsce-publish": "vsce publish"
  },
  "maintainers": [
    "Lars Heule"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Hic20/Nyx.git"
  },
  "bugs": {
    "url": "https://github.com/Hic20/nyx-theme/issues"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "Nyx.jpg",
  "contributes": {
    "themes": [
      {
        "label": "Nyx Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Nyx-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.1",
    "prettier": "^3.0.0",
    "tinycolor2": "^1.6.0",
    "vsce": "^1.97.0"
  }
}
